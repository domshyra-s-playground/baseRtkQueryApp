    @*<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.1/css/all.css" integrity="sha384-50oBUHEmvpQ+1lW4y57PTFmhCaXp0ML5d60M1M7uH2+nqUivzIebhndOJK28anvf" crossorigin="anonymous">*@

    <!--Inlcude min so we can use this before we render DOM elements...but JQuery is also included with the react stuff at the moment. So that at some point might need to be optimixed. For now here is the min to no break existing code-->
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"
            integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
            crossorigin="anonymous"></script>

</head>
<body>
    <header class="navbar navbar-expand-lg navbar-dark bg-primary">
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNav">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item">
                    <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">About me</a>
                    <div class="dropdown-menu" role="menu" aria-labelledby="AboutMeDropdown">
                        <a class="dropdown-item" href="@Url.Action("AboutMe")#about-me">About me</a>
                        <a class="dropdown-item" href="@Url.Action("AboutMe")#education">Education</a>
                        <a class="dropdown-item" href="@Url.Action("AboutMe")#work-experience">Work experience</a>
                    </div>

                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Projects</a>
                    <div class="dropdown-menu" role="menu" aria-labelledby="ProjectsDropdown">
                        <a class="dropdown-item" href="@Url.Action("Projects")#projects">Projects</a>
                        <div class="dropdown-divider"></div>
                        <a class="dropdown-item" href="@Url.Action("Projects")#urcs">URCS</a>
                        <a class="dropdown-item" href="@Url.Action("Projects")#loan-monitoring">Loan Monitoring</a>
                        <a class="dropdown-item" href="@Url.Action("Projects")#po-tracking">PO Tracker</a>
                        <a class="dropdown-item" href="@Url.Action("Projects")#corpool">Corpool</a>
                    </div>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Interests</a>
                    <div class="dropdown-menu" role="menu" aria-labelledby="IntrestsDropdown">
                        <a class="dropdown-item" href="@Url.Action("Interests")#intrests">Interests</a>
                        <a class="dropdown-item" href="@Url.Action("Interests")#hobbies">Hobbies</a>
                    </div>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Skills</a>
                    <div class="dropdown-menu" role="menu" aria-labelledby="SkillsDropdown">
                        <a class="dropdown-item" href="@Url.Action("Skills")#skills">Skills</a>
                    </div>
                </li>
            </ul>
            <ul class="navbar-nav flex-row ml-md-auto d-none d-md-flex">
                <li class="nav-item">
                    <a class="nav-link p-2" href="https://docs.google.com/document/d/1oJxWjIQ_XCleTdJ7TdPlkLsLV6jr1B8h6BGWKzhayFI/edit?usp=sharing" target="_blank" rel="noopener" aria-label="Resume"
                       data-toggle="tooltip" data-placement="bottom" title="View my résumé">
                        <i class="far fa-file-word social-media"></i>
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link p-2" href="https://github.com/domshyra" target="_blank" rel="noopener" aria-label="GitHub"
                       data-toggle="tooltip" data-placement="bottom" title="View my GitHub">
                        <i class="fab fa-github social-media"></i>
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link p-2" href="https://www.linkedin.com/in/domshyra/" target="_blank" rel="noopener" aria-label="LinkedIn"
                       data-toggle="tooltip" data-placement="bottom" title="View my LinkedIn">
                        <i class="fab fa-linkedin-in social-media"></i>
                    </a>
                </li>
            </ul>
            <a class="btn btn-outline-light d-none d-lg-inline-block mb-3 mb-md-0 ml-md-3" href="mailto:domshyra@gmail.com?subject=Contacting you from your website">Contact</a>
        </div>
    </header>
    <div class="container body-content my-3 py-3">
        @RenderBody()
    </div>
    <div class="container mt-1 pt-3 border-top border-light">
        <footer>
            <p>&copy; @DateTime.Now.Year - dominic schira</p>
            <p>View this websites code on <a href="https://github.com/domshyra/domshyra" target="_blank">github</a>.</p>
        </footer>
    </div>
    <script src="~/js/bundle.js"></script>
</body>
</html>

<script>
    $(function () {
        $('[data-toggle="tooltip"]').tooltip()
    })
</script>