<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <UserSecretsId>498f3a18-c750-4b49-b20a-3adf9786aae8</UserSecretsId>
    <ApplicationIcon>wwwroot\favicon.ico</ApplicationIcon>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <TypeScriptToolsVersion>Latest</TypeScriptToolsVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile></DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="wwwroot\lib\font-awesome\**" />
    <Content Remove="wwwroot\lib\font-awesome\**" />
    <EmbeddedResource Remove="wwwroot\lib\font-awesome\**" />
    <None Remove="wwwroot\lib\font-awesome\**" />
  </ItemGroup>

  <ItemGroup>
    <Content Remove="wwwroot\src\tsconfig.json" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="webpack.config.js" />
  </ItemGroup>

  <ItemGroup>
    <Content Include=".babelrc" />
    <Content Include="node_modules\react-dom\build-info.json" />
    <Content Include="node_modules\react-dom\package.json" />
    <Content Include="node_modules\react\build-info.json" />
    <Content Include="node_modules\react\package.json" />
    <Content Include="webpack.config.js" />
  </ItemGroup>

  <ItemGroup>
    <None Include="node_modules\react-dom\cjs\react-dom-server.browser.development.js" />
    <None Include="node_modules\react-dom\cjs\react-dom-server.browser.production.min.js" />
    <None Include="node_modules\react-dom\cjs\react-dom-server.node.development.js" />
    <None Include="node_modules\react-dom\cjs\react-dom-server.node.production.min.js" />
    <None Include="node_modules\react-dom\cjs\react-dom-test-utils.development.js" />
    <None Include="node_modules\react-dom\cjs\react-dom-test-utils.production.min.js" />
    <None Include="node_modules\react-dom\cjs\react-dom.development.js" />
    <None Include="node_modules\react-dom\cjs\react-dom.production.min.js" />
    <None Include="node_modules\react-dom\cjs\react-dom.profiling.min.js" />
    <None Include="node_modules\react-dom\index.js" />
    <None Include="node_modules\react-dom\LICENSE" />
    <None Include="node_modules\react-dom\profiling.js" />
    <None Include="node_modules\react-dom\README.md" />
    <None Include="node_modules\react-dom\server.browser.js" />
    <None Include="node_modules\react-dom\server.js" />
    <None Include="node_modules\react-dom\server.node.js" />
    <None Include="node_modules\react-dom\test-utils.js" />
    <None Include="node_modules\react-dom\umd\react-dom-server.browser.development.js" />
    <None Include="node_modules\react-dom\umd\react-dom-server.browser.production.min.js" />
