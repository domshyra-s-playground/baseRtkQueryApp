    <None Include="node_modules\react-dom\umd\react-dom-test-utils.production.min.js" />
    <None Include="node_modules\react-dom\umd\react-dom.development.js" />
    <None Include="node_modules\react-dom\umd\react-dom.production.min.js" />
    <None Include="node_modules\react-dom\umd\react-dom.profiling.min.js" />
    <None Include="node_modules\react\cjs\react-jsx-dev-runtime.development.js" />
    <None Include="node_modules\react\cjs\react-jsx-dev-runtime.production.min.js" />
    <None Include="node_modules\react\cjs\react-jsx-dev-runtime.profiling.min.js" />
